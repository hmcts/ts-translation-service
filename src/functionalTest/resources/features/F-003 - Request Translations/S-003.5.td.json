{
  "_guid_": "S-003.5",
  "_extends_": "Translation_Cy_Base",
  "title": "must return a successful response when duplicate English phrases exists in translation request",

  "specs": [
    "an active solicitor profile"
  ],

  "users": {
    "_extends_": "NoInvokingUser"
  },

  "request": {
    "headers": {
      "ServiceAuthorization": "[[DEFAULT_AUTO_VALUE]]",
      "Content-Type": "application/json",
      "Accept": "application/json"
    },
    "body": {
      "phrases": [
        "English phrase with translation",
        "English phrase with translation",
        "English phrase with translation"
      ]
    }
  },

  "expectedResponse": {
    "_extends_": "Common_200_Response",
    "body": {
      "translations": {
        "English phrase with translation": "Welsh phrase one"
      }
    }
  }
}
