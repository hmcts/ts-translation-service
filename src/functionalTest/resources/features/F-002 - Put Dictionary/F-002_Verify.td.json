{
  "_guid_": "F-002_Verify",
  "_extends_": "Translation_Cy_Base",
  "title": "must return a 200 OK containing translations for supplied phrases",
  "specs": [
    "to verify translations"
  ],

  "users": {
    "invokingUser": {
      "_extends_": "ManageTranslationsUser"
    }
  },

  "request": {
    "_extends_": "Common_Request",
    "body": {
      "phrases": "${[scenarioContext][customValues][phrasesFromContextTranslations]}"
    }
  },

  "expectedResponse": {
    "_extends_": "Common_200_Response",
    "body": {
      "translations": "${[scenarioContext][customValues][containsTranslationsFromContextAsTranslateResponse]}"
    }
  }
}
